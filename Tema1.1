void setup()
{
  Serial.begin(9600);
  pinMode(13, OUTPUT);
  pinMode(12, OUTPUT);
  pinMode(11, OUTPUT);
}

void loop()
{
  if(Serial.available()>0)
  {
    String s= Serial.readString();
    Serial.print(s);
  if(s[0]=='2')
  {
    if(s[1]=='F' && s[6]=='F') //alb 2FFFFFF
    {
  digitalWrite(13, HIGH);
  digitalWrite(12, HIGH);
  digitalWrite(11, HIGH);
    }
  else if(s[1]=='F' && s[3]=='0' && s[6]=='F') //mov 2FF00FF
  {
  digitalWrite(13, HIGH);
  digitalWrite(12, LOW);
  digitalWrite(11, HIGH);
  }
    else  //rosu 2FF0000
    {
      digitalWrite(13, HIGH);
      digitalWrite(12, LOW);
      digitalWrite(11, LOW);
    }
  }
}
}
