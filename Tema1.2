void setup()
{
  Serial.begin(9600);
  pinMode(13,OUTPUT);
}
void loop()
{
  if(Serial.available()>0)
  {
    String s=Serial.readString();
    Serial.print(s);
    if(s[0]=='1')
    {
     if(s[1]=='A')
       digitalWrite(13, HIGH);
    else if(s[1]=='S') 
      digitalWrite(13, HIGH);
    } 
  }
}
